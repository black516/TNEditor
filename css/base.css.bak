#editor_wrap_1{border:1px solid #b7b7b7;position:absolute;}
#editor_wrap_2{border:3px solid #f5f5f5;}
#editMenu{overflow:hidden;padding:1px;}
#editMenu ul{margin:0;padding:0;background:#efefef;overflow:hidden;border-bottom:1px solid #ccc;padding:0 2px 0 2px;}
#editMenu ul li{float:left;list-style:none;margin-top:1px;margin-bottom:2px;}
#editMenu ul li:hover{background:#fff;}
#editMenu ul li a{display:block; width:16px;height:16px;float:left;border:1px solid #efefef;}
#editMenu ul li a:hover{border:1px solid #ddd;}



#editFooter{position:absolute;bottom:0;left:0;height:14px;font-size:12px;padding:2px;color:#333;border-top:1px solid #ddd;background:#fefefe;}